{
    "spec": "CordOSInstallablePackage",
    "specv": 1,
    "sdk": 1,
    "name": "Package Name Here",
    "version": "1.0.0",
    "author": "Author Name Here",
    "build": 1,
    "description": "Description Here",
    "package": {
        "zip": "https://github.com/dev/<repo>/archive/<branch>.zip   or    self"
    },
    "dependencies": {
        "system": {
            "version": ">=1.0",
            "build": 1
        }
    },
    "conflicts": {
        "system": {
            "version": "any",
            "build": 1
        }
    },
    "pip": [
        "package1==1.0.0",
        "package2"
    ],
    "packageMaps": {
        "kernel": {
            "modules": "MODULES",
            "services": "SERVICES",
            "commands": "COMMANDS"
        },
        "user": {
            "modules": "MODULES-USER",
            "services": "SERVICES-USER",
            "commands": "COMMANDS-USER"
        },
        "data": [
            {
                "source": "data/files/thispackage",
                "destination": "data/files/thispackage"
            }
        ]
    },
    "taskmap": [
        {
            "task": "cleanup",
            "target": [
                "data/files/thispackagecache"
            ],
            "ignoreFail": true
        },
        {
            "task": "execute",
            "target": "data/files/thispackage/install.sh"
        },
        {
            "task": "install"
        },
        {
            "task": "copy",
            "source": [
                "$SOURCE/data/files/thispackage"
            ],
            "destination": [
                "data/files/thispackage"
            ]
        }
    ]
}